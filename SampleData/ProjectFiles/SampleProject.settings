                <DefaultOutputDpiX>614</DefaultOutputDpiX>
                <DefaultOutputDpiY>614</DefaultOutputDpiY>
            </LayerBlockElementDefinition>

            <LayerBlockElementDefinition>
                <Categories>Placeholders</Categories>
                <Type>9907</Type>
                <IconFullPath>PlaceholderTextures/rectangle_orange.png</IconFullPath>
                <DefaultScaleX>1</DefaultScaleX>
                <DefaultScaleY>1</DefaultScaleY>
                <DefaultPivotX>0</DefaultPivotX>
                <DefaultPivotY>0</DefaultPivotY>
                <DefaultOutputDpiX>614</DefaultOutputDpiX>
                <DefaultOutputDpiY>614</DefaultOutputDpiY>
            </LayerBlockElementDefinition>

            <LayerBlockElementDefinition>
                <Categories>Placeholders</Categories>
                <Type>9908</Type>
                <IconFullPath>PlaceholderTextures/rectangle_purple.png</IconFullPath>
                <DefaultScaleX>1</DefaultScaleX>
                <DefaultScaleY>1</DefaultScaleY>
                <DefaultPivotX>0</DefaultPivotX>
                <DefaultPivotY>0</DefaultPivotY>
                <DefaultOutputDpiX>614</DefaultOutputDpiX>
                <DefaultOutputDpiY>614</DefaultOutputDpiY>
            </LayerBlockElementDefinition>

            <LayerBlockElementDefinition>
                <Categories>Placeholders</Categories>
                <Type>9909</Type>
                <IconFullPath>PlaceholderTextures/rectangle_red.png</IconFullPath>
                <DefaultScaleX>1</DefaultScaleX>
                <DefaultScaleY>1</DefaultScaleY>
                <DefaultPivotX>0</DefaultPivotX>
